 * @author Jason Reynolds
 * @param
 * @returns
 * 
 * Function unit test for power. Includes a test for matrices and numbers.
 */

*import math:power;

function power_test() {

    let a = 3;
    let b = 4;
    if (power(b,a) !== 64) {
        throw new Error('Unit test failed for numbers power test');
    }
    let c = new Mat([[3,2],[4,1]]);
    if (! (power(c, a) === [[27,8],[64,1]] )) {
        throw new Error('power test failed for matrices');
    }
 
 }