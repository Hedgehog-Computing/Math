/**
 * @author Jason Reynolds
 * @param 
 * @returns
 * 
 * Function for testing factorial.hhs
 */


function factorial_test() {

    *import math: factorial

    if (!(factorial(5) === 120)) {
        throw new Error('factorial unit test failed for number.');
    }

    if (!(factorial([[3, 2], [5, 1]]) === [[6, 2], [120, 1]])) {
        throw new Error('factorial unit test failed for array.');
    }
} 